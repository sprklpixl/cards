<h1>Welcome to Lorcana Database</h1>
<h2>Cards</h2>
<table>
  <thead>
    <tr>
      <th>Card ID</th>
      <th>Title</th>
      <th>Category</th>
    </tr>
  </thead>
  <tbody>
    <% @products.each do |product| %>
      <tr>
        <td><%= link_to product.product_id, product_path(product) %></td>
        <td><%= link_to product.title, product_path(product) %></td>
        <td><%= product.category.name if product.category %></td>
      </tr>
    <% end %>
  </tbody>
</table>